{
  "name": "CodeQL Configuration",
  "version": "2.1.0",
  "paths-ignore": [
    "ios/App/App/public/",
    "ios/App/App/www/",
    "ios/App/App/public/**",
    "ios/App/App/www/**",
    "android/app/src/main/assets/www/",
    "android/app/src/main/assets/www/**",
    "node_modules/",
    "node_modules/**",
    "dist/",
    "dist/**",
    "web-build/",
    "web-build/**",
    ".expo/",
    ".expo/**",
    "*.tsbuildinfo",
    "build/",
    "build/**",
    "coverage/",
    "coverage/**",
    ".nyc_output/",
    ".nyc_output/**"
  ],
  "queries": {
    "exclude": [
      "security-and-quality"
    ]
  },
  "packs": {
    "javascript": [
      "codeql/javascript-queries:security-and-quality"
    ]
  },
  "disable-default-rules": false,
  "disable-default-queries": false
}
